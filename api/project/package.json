{
  "name": "sources",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "check": "tsc --noEmit",
    "exec": "ts-node -r tsconfig-paths/register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "inversify": "^5.0.5",
    "js-yaml": "^4.0.0",
    "moment": "^2.29.1",
    "postgres": "^1.0.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.11",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.31",
    "axios": "^0.21.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(xxxx.*?\\.js$))"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "@test/(.+)$": "<rootDir>/src/test/$1",
      "@core/(.+)$": "<rootDir>/src/main/core/$1",
      "@common/(.+)$": "<rootDir>/src/main/common/$1",
      "@controllers/(.+)$": "<rootDir>/src/main/controllers/$1",
      "@services/(.+)$": "<rootDir>/src/main/services/$1"
    }
  }
}
