{
  "name": "sources",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "check": "tsc --noEmit",
    "exec": "ts-node -r tsconfig-paths/register",
    "start": "NODE_ENV=development ts-node -r tsconfig-paths/register -- src/main/start.ts",
    "start:prod": "NODE_ENV=production ts-node -r tsconfig-paths/register -- src/main/start.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "js-yaml": "^4.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "moment": "^2.29.4",
    "postgres": "^3.3.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.snakecase": "^4.1.7",
    "@types/lodash.uniq": "^4.5.7",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.2.5",
    "axios": "^1.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.5.0",
    "prisma": "^4.14.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(xxxx.*?\\.js$))"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "globalSetup": "<rootDir>/src/test/setup.ts",
    "globalTeardown": "<rootDir>/src/test/teardown.ts",
    "moduleNameMapper": {
      "@test/(.+)$": "<rootDir>/src/test/$1",
      "@core/(.+)$": "<rootDir>/src/main/core/$1",
      "@common/(.+)$": "<rootDir>/src/main/common/$1",
      "@controllers/(.+)$": "<rootDir>/src/main/controllers/$1",
      "@services/(.+)$": "<rootDir>/src/main/services/$1"
    }
  }
}
